{
	"servers": {
		"aspire": {
			"type": "stdio",
			"command": "aspire",
			"args": ["mcp", "start"]
		},
		"hex1b": {
			"type": "stdio",
			"command": "dnx",
			"args": [
				"Hex1b.McpServer@0.47.0",
				"--yes"
			]
		},
        "github": {
            "type": "http",
            "url": "https://api.githubcopilot.com/mcp/"
        },
		"NuGet": {
            "type": "stdio",
            "command": "dnx",
            "args": ["NuGet.Mcp.Server@1.1.24", "--yes"]
        }
	},
	"inputs": []
}
