{
	"servers": {
		"aspire": {
			"type": "stdio",
			"command": "dotnet",
			"args": ["run", "--project", "${workspaceFolder}/src/Aspire.Cli/Aspire.Cli.csproj", "--", "mcp", "start"]
		},
		"aspire-debug": {
			"type": "stdio",
			"command": "dotnet",
			"args": ["run", "--project", "${workspaceFolder}/src/Aspire.Cli/Aspire.Cli.csproj", "--", "mcp", "start"],
			"env": {
				"ASPIRE_MCP_DEBUG": "1"
			}
		},
		"aspire-release": {
			"type": "stdio",
			"command": "aspire",
			"args": ["mcp", "start"]
		},
		"hex1b": {
			"type": "stdio",
			"command": "dnx",
			"args": [
				"Hex1b.McpServer@0.47.0",
				"--yes"
			]
		},
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/"
     },
	},
	"inputs": []
}
