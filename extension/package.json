{
  "name": "aspire-vscode",
  "displayName": "Aspire",
  "description": "%extension.description%",
  "publisher": "microsoft-aspire",
  "version": "1.0.0",
  "aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
  "icon": "dotnet-aspire-logo-128.png",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "bugs": {
    "url": "https://github.com/dotnet/aspire/issues"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "dotnet",
    "orchestrator",
    "apphost"
  ],
  "activationEvents": [
    "onDebugResolve:aspire",
    "onDebugInitialConfigurations:aspire",
    "onDebugDynamicConfigurations:aspire",
    "workspaceContains:**/*.csproj",
    "onView:workbench.view.debug",
    "workspaceContains:**/apphost.cs"
  ],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "debuggers": [
      {
        "type": "aspire",
        "label": "Aspire",
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "%extension.debug.program%",
                "default": "${workspaceFolder}"
              },
              "debuggers": {
                "type": "object",
                "description": "%extension.debug.debuggers%",
                "default": {}
              }
            }
          }
        },
        "configurationSnippets": [
          {
            "label": "%extension.debug.defaultConfiguration.name%",
            "description": "%extension.debug.defaultConfiguration.description%",
            "body": {
              "type": "aspire",
              "request": "launch",
              "name": "%extension.debug.defaultConfiguration.name%",
              "program": "${workspaceFolder}"
            }
          }
        ],
        "initialConfigurations": [
          {
            "type": "aspire",
            "request": "launch",
            "name": "%extension.debug.defaultConfiguration.name%",
            "program": "${workspaceFolder}"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "aspire-vscode.add",
        "title": "%command.add%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.new",
        "title": "%command.new%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.init",
        "title": "%command.init%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.publish",
        "title": "%command.publish%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.update",
        "title": "%command.update%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.openTerminal",
        "title": "%command.openTerminal%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.config",
        "title": "%command.config%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.deploy",
        "title": "%command.deploy%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.configureLaunchJson",
        "title": "%command.configureLaunchJson%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.settings",
        "title": "%command.settings%",
        "category": "Aspire"
      },
      {
        "command": "aspire-vscode.runAppHost",
        "title": "%command.runAppHost%",
        "category": "Aspire",
        "icon": "$(run-all)"
      },
      {
        "command": "aspire-vscode.debugAppHost",
        "title": "%command.debugAppHost%",
        "category": "Aspire",
        "icon": "$(debug-all)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "aspire-vscode.runAppHost",
          "when": "resourceExtname == .cs && resourceFilename =~ /apphost\\.cs$/i",
          "group": "aspire_actions@1"
        },
        {
          "command": "aspire-vscode.debugAppHost",
          "when": "resourceExtname == .cs && resourceFilename =~ /apphost\\.cs$/i",
          "group": "aspire_actions@2"
        }
      ],
      "editor/title/run": [
        {
          "command": "aspire-vscode.runAppHost",
          "when": "(aspire.fileIsAppHostCs || aspire.workspaceHasAppHost) && aspire.editorSupportsRunDebug",
          "group": "navigation@-4"
        },
        {
          "command": "aspire-vscode.debugAppHost",
          "when": "(aspire.fileIsAppHostCs || aspire.workspaceHasAppHost) && aspire.editorSupportsRunDebug",
          "group": "navigation@-3"
        }
      ],
      "commandPalette": [
        {
          "command": "aspire-vscode.runAppHost",
          "when": "false"
        },
        {
          "command": "aspire-vscode.debugAppHost",
          "when": "false"
        }
      ]
    },
    "configuration": {
      "title": "Aspire",
      "properties": {
        "aspire.enableSettingsFileCreationPromptOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.aspire.enableSettingsFileCreationPromptOnStartup%",
          "scope": "window"
        },
        "aspire.aspireCliExecutablePath": {
          "type": "string",
          "default": "",
          "description": "%configuration.aspire.aspireCliExecutablePath%",
          "scope": "machine-overridable"
        },
        "aspire.enableAspireCliDebugLogging": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.aspire.enableAspireCliDebugLogging%",
          "scope": "window"
        },
        "aspire.enableAspireDcpDebugLogging": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.aspire.enableAspireDcpDebugLogging%",
          "scope": "window"
        },
        "aspire.enableAspireDashboardAutoLaunch": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.aspire.enableAspireDashboardAutoLaunch%",
          "scope": "window"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dotnet/aspire",
    "branch": "main"
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "unit-test": "vscode-test",
    "test": "yarn unit-test",
    "l10n:export": "gulp prepare",
    "l10n:import": "gulp localizationBundle",
    "precompile": "yarn l10n:export && yarn l10n:import",
    "prewatch": "yarn l10n:export && yarn l10n:import",
    "localize": "yarn l10n:export && yarn l10n:import"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-forge": "^1.3.11",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.98.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.3.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.27.0",
    "gulp": "^5.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^7.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "postcss": "^8.5.6",
    "sinon": "^21.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "wait-for-expect": "^4.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@vscode/extension-telemetry": "^1.0.0",
    "express": "^5.1.0",
    "jsonc-parser": "^3.3.1",
    "node-forge": "^1.3.1",
    "vscode-jsonrpc": "^8.2.1",
    "ws": "^8.18.3"
  },
  "resolutions": {
    "braces": "^3.0.3"
  }
}
