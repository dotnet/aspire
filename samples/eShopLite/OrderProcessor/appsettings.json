{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Azure": "Warning"
    }
  },

  "Aspire": {
    "Azure": {
      "Messaging": {
        "ServiceBus": {
          "OrderQueueName": "orders",
          "ClientOptions": {
            "RetryOptions": {
              "MaxRetries": 2,
              "Delay": "00:00:01"
            }
          },
          "Tracing": true
        }
      }
    }
  }
}
