{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Azure": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Aspire": {
    "Azure:Messaging:ServiceBus": {
      "OrderQueueName": "orders",
      "ClientOptions": {
        "RetryOptions": {
          "MaxRetries": 2,
          "Delay": "00:00:01"
        }
      },
      "Tracing": true
    },
    "StackExchange:Redis": {
      "ConfigurationOptions": {
        "AbortOnConnectFail": false,
        "ConnectRetry": 2
      }
    }
  }
}
