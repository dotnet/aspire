<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="it" original="../Resources.resx">
    <body>
      <trans-unit id="ApplicationNotStartedExceptionMessage">
        <source>The application must be started before resolving endpoints or connection strings.</source>
        <target state="translated">È necessario avviare l'applicazione prima di risolvere endpoint o stringhe di connessione.</target>
        <note />
      </trans-unit>
      <trans-unit id="EndpointForResourceNotFoundExceptionMessage">
        <source>Endpoint '{0}' for resource '{1}' not found.</source>
        <target state="translated">L'endpoint '{0}' per la risorsa '{1}' non è stato trovato.</target>
        <note />
      </trans-unit>
      <trans-unit id="ResourceDoesNotExposeConnectionStringExceptionMessage">
        <source>Resource '{0}' does not expose a connection string.</source>
        <target state="translated">La risorsa '{0}' non espone una stringa di connessione.</target>
        <note />
      </trans-unit>
      <trans-unit id="ResourceHasNoAllocatedEndpointsExceptionMessage">
        <source>Resource '{0}' has no allocated endpoints.</source>
        <target state="translated">La risorsa '{0}' non ha endpoint allocati.</target>
        <note />
      </trans-unit>
      <trans-unit id="ResourceNotFoundExceptionMessage">
        <source>Resource '{0}' not found.</source>
        <target state="translated">Risorsa '{0}' non trovata.</target>
        <note />
      </trans-unit>
    </body>
  </file>
