<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>$(NetCurrent)</TargetFramework>
    <IsPackable>true</IsPackable>
    <PackageTags>$(ComponentCachePackageTags) distributedcaching distributedcache redis</PackageTags>
    <Description>
      Registers an IDistributedCache in the DI container that connects to a
      Redis server. See Distributed Caching for more information. Enables
      corresponding health check, logging, and telemetry.
    </Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Aspire.StackExchange.Redis\Aspire.StackExchange.Redis.csproj" />
    <PackageReference Include="Microsoft.Extensions.Caching.StackExchangeRedis" />
  </ItemGroup>

</Project>
