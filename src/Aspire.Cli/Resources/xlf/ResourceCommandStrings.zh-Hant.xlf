<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="zh-Hant" original="../ResourceCommandStrings.resx">
    <body>
      <trans-unit id="CommandDescription">
        <source>Execute a command on a resource.</source>
        <target state="translated">在資源上執行命令。</target>
        <note />
      </trans-unit>
      <trans-unit id="CommandNameArgumentDescription">
        <source>The name of the command to execute.</source>
        <target state="translated">所要執行命令的名稱。</target>
        <note />
      </trans-unit>
      <trans-unit id="CommandResourceArgumentDescription">
        <source>The name of the resource to execute the command on.</source>
        <target state="translated">要執行命令的資源名稱。</target>
        <note />
      </trans-unit>
      <trans-unit id="OptionNotValidWithResource">
        <source>The '{0}' option is not valid when starting a resource.</source>
        <target state="translated">啟動資源時，'{0}' 選項無效。</target>
        <note />
      </trans-unit>
      <trans-unit id="RestartDescription">
        <source>Restart a running resource.</source>
        <target state="translated">重新啟動正在執行的資源。</target>
        <note />
      </trans-unit>
      <trans-unit id="RestartResourceArgumentDescription">
        <source>The name of the resource to restart.</source>
        <target state="translated">要重新啟動的資源名稱。</target>
        <note />
      </trans-unit>
      <trans-unit id="SelectAppHostAction">
        <source>connect to</source>
        <target state="translated">連線至</target>
        <note />
      </trans-unit>
      <trans-unit id="StartDescription">
        <source>Start an apphost in the background, or start a stopped resource.</source>
        <target state="translated">在背景啟動 apphost，或啟動已停止的資源。</target>
        <note />
      </trans-unit>
      <trans-unit id="StartResourceArgumentDescription">
        <source>The name of the resource to start. If not specified, starts the AppHost in the background.</source>
        <target state="translated">要啟動的資源名稱。若未指定，則會在背景啟動 AppHost。</target>
        <note />
      </trans-unit>
    </body>
  </file>
</xliff>