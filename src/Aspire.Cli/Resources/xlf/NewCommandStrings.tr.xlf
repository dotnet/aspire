<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="tr" original="../NewCommandStrings.resx">
    <body>
      <trans-unit id="Description">
        <source>Create a new Aspire project.</source>
        <target state="translated">Yeni bir Aspire projesi oluşturun.</target>
        <note />
      </trans-unit>
      <trans-unit id="EnterTheOutputPath">
        <source>Enter the output path:</source>
        <target state="translated">Çıkış yolunu girin:</target>
        <note />
      </trans-unit>
      <trans-unit id="EnterTheProjectName">
        <source>Enter the project name</source>
        <target state="new">Enter the project name</target>
        <note />
      </trans-unit>
      <trans-unit id="InvalidProjectName">
        <source>Invalid project name.</source>
        <target state="translated">Geçersiz proje adı.</target>
        <note />
      </trans-unit>
      <trans-unit id="NameArgumentDescription">
        <source>The name of the project to create.</source>
        <target state="translated">Oluşturulacak projenin adı.</target>
        <note />
      </trans-unit>
      <trans-unit id="OutputArgumentDescription">
        <source>The output path for the project.</source>
        <target state="translated">Projenin çıkış yolu.</target>
        <note />
      </trans-unit>
      <trans-unit id="SelectAProjectTemplate">
        <source>Select a project template:</source>
        <target state="translated">Bir proje şablonu seçin:</target>
        <note />
      </trans-unit>
      <trans-unit id="SelectATemplateVersion">
        <source>Select a template version:</source>
        <target state="translated">Bir şablon sürümü seçin:</target>
        <note />
      </trans-unit>
      <trans-unit id="SourceArgumentDescription">
        <source>The NuGet source to use for the project templates.</source>
        <target state="translated">Proje şablonları için kullanılacak NuGet kaynağı.</target>
        <note />
      </trans-unit>
      <trans-unit id="UsePrereleaseTemplates">
        <source>(use pre-release templates)</source>
        <target state="new">(use pre-release templates)</target>
        <note />
      </trans-unit>
      <trans-unit id="VersionArgumentDescription">
        <source>The version of the project templates to use.</source>
        <target state="translated">Kullanılacak proje şablonlarının sürümü.</target>
        <note />
      </trans-unit>
    </body>
