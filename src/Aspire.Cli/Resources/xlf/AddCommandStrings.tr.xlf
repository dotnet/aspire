<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="tr" original="../AddCommandStrings.resx">
    <body>
      <trans-unit id="AddingAspireIntegration">
        <source>Adding Aspire integration...</source>
        <target state="translated">Aspire tümleştirmesi ekleniyor...</target>
        <note />
      </trans-unit>
      <trans-unit id="Description">
        <source>Add an integration to the Aspire project.</source>
        <target state="translated">Aspire projesine bir tümleştirme ekleyin.</target>
        <note />
      </trans-unit>
      <trans-unit id="ErrorOccurredWhileAddingPackage">
        <source>An error occurred while adding the package: {0}</source>
        <target state="translated">{0} paketi eklenirken bir hata oluştu</target>
        <note />
      </trans-unit>
      <trans-unit id="IntegrationArgumentDescription">
        <source>The name of the integration to add (e.g. redis, postgres).</source>
        <target state="translated">Eklenecek tümleştirmenin adı (ör. redis, postgres).</target>
        <note />
      </trans-unit>
      <trans-unit id="NoIntegrationPackagesFound">
        <source>No integration packages were found. Please check your internet connection or NuGet source configuration.</source>
        <target state="translated">Hiçbir tümleştirme paketi bulunamadı. Lütfen internet bağlantınızı veya NuGet kaynak yapılandırmanızı kontrol edin.</target>
        <note />
      </trans-unit>
      <trans-unit id="NoPackagesFound">
        <source>No packages found.</source>
        <target state="translated">Paket bulunamadı.</target>
        <note />
      </trans-unit>
      <trans-unit id="NoPackagesMatchedSearchTerm">
        <source>No packages matched your search term '{0}'. Showing all available packages.</source>
        <target state="translated">'{0}' arama teriminizle eşleşen herhangi bir paket bulunamadı. Kullanılabilir tüm paketler gösteriliyor.</target>
        <note>{0} is the search term provided by the user.</note>
      </trans-unit>
      <trans-unit id="PackageAddedSuccessfully">
        <source>The package {0}::{1} was added successfully.</source>
        <target state="translated">{0}::{1} paketi başarıyla eklendi.</target>
        <note>{0} is the package name, {1} is the version.</note>
      </trans-unit>
      <trans-unit id="PackageInstallationFailed">
        <source>The package installation failed with exit code {0}. For more information run with --debug switch.</source>
        <target state="translated">Paket yüklemesi {0} çıkış koduyla başarısız oldu. Daha fazla bilgi için --debug anahtarıyla çalıştırın.</target>
        <note />
      </trans-unit>
      <trans-unit id="ProjectArgumentDescription">
        <source>The path to the project file to add the integration to.</source>
        <target state="translated">Tümleştirmenin ekleneceği proje dosyasının yolu.</target>
        <note />
      </trans-unit>
      <trans-unit id="SearchingForAspirePackages">
        <source>Searching for Aspire packages...</source>
        <target state="translated">Aspire paketleri aranıyor...</target>
        <note />
      </trans-unit>
      <trans-unit id="SelectAVersionOfPackage">
        <source>Select a version of {0}:</source>
        <target state="translated">Bir {0} sürümü seçin:</target>
        <note>{0} is the package name.</note>
      </trans-unit>
      <trans-unit id="SelectAnIntegrationToAdd">
        <source>Select an integration to add:</source>
        <target state="translated">Eklemek için bir tümleştirme seçin:</target>
        <note />
      </trans-unit>
      <trans-unit id="SourceArgumentDescription">
        <source>The NuGet source to use for the integration.</source>
        <target state="translated">Tümleştirme için kullanılacak NuGet kaynağı.</target>
        <note />
      </trans-unit>
      <trans-unit id="UnexpectedNumberOfPackagesFound">
        <source>Unexpected number of packages found.</source>
        <target state="translated">Beklenmeyen sayıda paket bulundu.</target>
        <note />
      </trans-unit>
      <trans-unit id="UsePrereleasePackages">
        <source>Use pre-release packages</source>
        <target state="translated">Ön sürüm paketlerini kullanın</target>
        <note />
      </trans-unit>
      <trans-unit id="VersionArgumentDescription">
        <source>The version of the integration to add.</source>
        <target state="translated">Eklenecek tümleştirmenin sürümü.</target>
        <note />
      </trans-unit>
    </body>
  </file>
</xliff>