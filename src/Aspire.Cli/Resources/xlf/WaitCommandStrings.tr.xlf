<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="tr" original="../WaitCommandStrings.resx">
    <body>
      <trans-unit id="Description">
        <source>Wait for a resource to reach a target status.</source>
        <target state="translated">Bir kaynağın hedef duruma ulaşmasını bekleyin.</target>
        <note />
      </trans-unit>
      <trans-unit id="InvalidStatusValue">
        <source>Invalid status value '{0}'. Valid values are: healthy, up, down.</source>
        <target state="translated">Geçersiz durum değeri '{0}'. Geçerli değerler: healthy, up, down.</target>
        <note />
      </trans-unit>
      <trans-unit id="NoInScopeAppHostsShowingAll">
        <source>No running AppHosts found in the current directory. Showing all running AppHosts:</source>
        <target state="translated">Geçerli dizinde çalışan AppHost bulunamadı. Tüm çalışan AppHost'lar gösteriliyor:</target>
        <note />
      </trans-unit>
      <trans-unit id="NoRunningAppHostsFound">
        <source>No running AppHosts found.</source>
        <target state="translated">Çalışan AppHost bulunamadı.</target>
        <note />
      </trans-unit>
      <trans-unit id="ProjectOptionDescription">
        <source>The path to the Aspire AppHost project file.</source>
        <target state="translated">Aspire AppHost proje dosyasının yolu.</target>
        <note />
      </trans-unit>
      <trans-unit id="ResourceArgumentDescription">
        <source>The name of the resource to wait for.</source>
        <target state="translated">Beklenecek kaynağın adı.</target>
        <note />
      </trans-unit>
      <trans-unit id="ResourceEnteredFailedState">
        <source>Resource '{0}' entered a failed state: {1}.</source>
        <target state="translated">'{0}' kaynağı başarısız bir duruma girdi: {1}.</target>
        <note />
      </trans-unit>
      <trans-unit id="ResourceNotFound">
        <source>Resource '{0}' was not found.</source>
        <target state="translated">'{0}' kaynağı bulunamadı.</target>
        <note />
      </trans-unit>
      <trans-unit id="ResourceReachedTargetStatus">
        <source>Resource '{0}' is {1}. ({2:F1}s)</source>
        <target state="translated">Kaynak '{0}' {1}‘dir. ({2:F1} sn)</target>
        <note />
      </trans-unit>
      <trans-unit id="ScanningForRunningAppHosts">
        <source>Scanning for running AppHosts...</source>
        <target state="translated">Çalışan AppHost'lar taranıyor...</target>
        <note />
      </trans-unit>
      <trans-unit id="SelectAppHost">
        <source>Select an AppHost:</source>
        <target state="translated">AppHost seçin:</target>
        <note />
      </trans-unit>
      <trans-unit id="StatusOptionDescription">
        <source>The target status to wait for (healthy, up, down). Defaults to healthy.</source>
        <target state="translated">Beklenecek hedef durum (healthy, up, down). Varsayılan değer healthy'dir.</target>
        <note />
      </trans-unit>
      <trans-unit id="TimeoutMustBePositive">
        <source>Timeout must be a positive number of seconds.</source>
        <target state="translated">Zaman aşımı pozitif bir saniye sayısı olmalıdır.</target>
        <note />
      </trans-unit>
      <trans-unit id="TimeoutOptionDescription">
        <source>Maximum time to wait in seconds. Defaults to 120.</source>
        <target state="translated">Bekleme süresinin maksimum süresi saniye cinsindendir. Varsayılan değer 120'tür.</target>
        <note />
      </trans-unit>
      <trans-unit id="WaitTimedOut">
        <source>Timed out waiting for resource '{0}' to be {1} after {2}s.</source>
        <target state="translated">'{0}' kaynağının {2} saniye sonra {1} olması beklenirken zaman aşımı oluştu.</target>
        <note />
      </trans-unit>
      <trans-unit id="WaitingForResource">
        <source>Waiting for resource '{0}' to be {1}...</source>
        <target state="translated">'{0}' kaynağının {1} olması bekleniyor...</target>
        <note />
      </trans-unit>
    </body>
  </file>
</xliff>