        <target state="translated">Belirtilen proje dosyası bir Aspire uygulama ana işlemi projesi değil.</target>
        <note />
      </trans-unit>
      <trans-unit id="StoppingAspire">
        <source>Stopping Aspire.</source>
        <target state="translated">Aspire durduruluyor.</target>
        <note />
      </trans-unit>
