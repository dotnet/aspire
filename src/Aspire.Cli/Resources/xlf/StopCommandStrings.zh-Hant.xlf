<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="zh-Hant" original="../StopCommandStrings.resx">
    <body>
      <trans-unit id="AllOptionDescription">
        <source>Stop all running AppHosts.</source>
        <target state="new">Stop all running AppHosts.</target>
        <note />
      </trans-unit>
      <trans-unit id="AppHostStoppedSuccessfully">
        <source>AppHost stopped successfully.</source>
        <target state="translated">已成功停止 AppHost。</target>
        <note />
      </trans-unit>
      <trans-unit id="Description">
        <source>Stop a running apphost or the specified resource.</source>
        <target state="translated">停止正在執行的 Aspire apphost。</target>
        <note />
      </trans-unit>
      <trans-unit id="FailedToStopAppHost">
        <source>Failed to stop the AppHost.</source>
        <target state="translated">無法停止 AppHost。</target>
        <note />
      </trans-unit>
      <trans-unit id="MultipleAppHostsNonInteractive">
        <source>Multiple AppHosts are running. Use --project to specify which one to stop, or use --all to stop all of them.</source>
        <target state="new">Multiple AppHosts are running. Use --project to specify which one to stop, or use --all to stop all of them.</target>
        <note />
      </trans-unit>
      <trans-unit id="MultipleAppHostsRunning">
        <source>Multiple AppHosts are running. Use --project to specify which one to stop, or select one:</source>
        <target state="translated">多個 AppHost 正在執行。請使用 --project 指定要停止的 AppHost，或從中選取一個:</target>
        <note />
      </trans-unit>
      <trans-unit id="NoInScopeAppHostsShowingAll">
        <source>No running AppHosts found in the current directory. Showing all running AppHosts:</source>
        <target state="translated">目前目錄中找不到正在執行的 AppHost。顯示所有正在執行的 AppHost:</target>
        <note />
      </trans-unit>
      <trans-unit id="NoRunningAppHostsFound">
        <source>No running AppHosts found in scope.</source>
        <target state="translated">在範圍內找不到正在執行的 AppHost。</target>
        <note />
      </trans-unit>
      <trans-unit id="ProjectArgumentDescription">
        <source>The path to the Aspire AppHost project file.</source>
        <target state="translated">Aspire AppHost 專案檔案的路徑。</target>
        <note />
      </trans-unit>
      <trans-unit id="ScanningForRunningAppHosts">
        <source>Scanning for running AppHosts...</source>
        <target state="translated">正在掃描執行中的 AppHost...</target>
        <note />
      </trans-unit>
      <trans-unit id="SelectAppHostToStop">
        <source>Select an AppHost to stop:</source>
        <target state="translated">選取要停止的 AppHost:</target>
        <note />
      </trans-unit>
      <trans-unit id="StoppingAppHost">
        <source>Stopping Aspire apphost...</source>
        <target state="translated">正在停止 Aspire AppHost...</target>
        <note />
      </trans-unit>
    </body>
  </file>
</xliff>