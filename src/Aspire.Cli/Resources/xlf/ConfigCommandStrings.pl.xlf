<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="pl" original="../ConfigCommandStrings.resx">
    <body>
      <trans-unit id="ConfigurationKeyDeletedGlobally">
        <source>Configuration '{0}' deleted globally.</source>
        <target state="translated">Konfiguracja „{0}” została usunięta globalnie.</target>
        <note />
      </trans-unit>
      <trans-unit id="ConfigurationKeyDeletedLocally">
        <source>Configuration '{0}' deleted locally.</source>
        <target state="translated">Konfiguracja „{0}” została usunięta lokalnie.</target>
        <note />
      </trans-unit>
      <trans-unit id="ConfigurationKeySetGlobally">
        <source>Configuration '{0}' set to '{1}' globally.</source>
        <target state="translated">Konfiguracja „{0}” ustawiona globalnie jako „{1}”.</target>
        <note>{0} is the key, {1} is the value</note>
      </trans-unit>
      <trans-unit id="ConfigurationKeySetLocally">
        <source>Configuration '{0}' set to '{1}' locally.</source>
        <target state="translated">Konfiguracja „{0}” jest ustawiona lokalnie jako „{1}”.</target>
        <note>{0} is the key, {1} is the value</note>
      </trans-unit>
      <trans-unit id="DeleteCommand_Description">
        <source>Delete a configuration value.</source>
        <target state="translated">Usuń wartość konfiguracji.</target>
        <note />
      </trans-unit>
      <trans-unit id="DeleteCommand_GlobalArgumentDescription">
        <source>Delete the configuration value from the global $HOME/.aspire/settings.json instead of the local settings file.</source>
        <target state="translated">Usuń wartość konfiguracji z globalnego pliku $HOME/.settings.json zamiast z pliku ustawień lokalnych.</target>
        <note />
      </trans-unit>
      <trans-unit id="DeleteCommand_KeyArgumentDescription">
        <source>The configuration key to delete.</source>
        <target state="translated">Klucz konfiguracji do usunięcia.</target>
        <note />
      </trans-unit>
      <trans-unit id="Description">
        <source>Manage configuration settings.</source>
        <target state="translated">Zarządzaj ustawieniami konfiguracji.</target>
        <note />
      </trans-unit>
      <trans-unit id="GetCommand_Description">
        <source>Get a configuration value.</source>
        <target state="translated">Pobierz wartość konfiguracji.</target>
        <note />
      </trans-unit>
      <trans-unit id="GetCommand_KeyArgumentDescription">
        <source>The configuration key to retrieve.</source>
        <target state="translated">Klucz konfiguracji do pobrania.</target>
        <note />
      </trans-unit>
      <trans-unit id="NoConfigurationValuesFound">
        <source>No configuration values found.</source>
        <target state="translated">Nie znaleziono wartości konfiguracji.</target>
        <note />
      </trans-unit>
      <trans-unit id="SetCommand_Description">
        <source>Set a configuration value.</source>
        <target state="translated">Ustaw wartość konfiguracji.</target>
        <note />
      </trans-unit>
      <trans-unit id="SetCommand_GlobalArgumentDescription">
        <source>Set the configuration value globally in $HOME/.aspire/settings.json instead of the local settings file.</source>
        <target state="translated">Ustaw wartość konfiguracji globalnie w pliku $HOME/.settings.json zamiast w pliku ustawień lokalnych.</target>
        <note />
      </trans-unit>
      <trans-unit id="SetCommand_KeyArgumentDescription">
        <source>The configuration key to set.</source>
        <target state="translated">Klucz konfiguracji do ustawienia.</target>
        <note />
      </trans-unit>
      <trans-unit id="SetCommand_ValueArgumentDescription">
        <source>The configuration value to set.</source>
        <target state="translated">Wartość konfiguracji do ustawienia.</target>
        <note />
      </trans-unit>
      <trans-unit id="ShowCommand_Description">
        <source>Show effective configuration as merged JSON.</source>
        <target state="new">Show effective configuration as merged JSON.</target>
        <note />
      </trans-unit>
      <trans-unit id="ShowCommand_UglyOptionDescription">
        <source>Output plain ASCII without ANSI formatting or colors.</source>
        <target state="new">Output plain ASCII without ANSI formatting or colors.</target>
        <note />
      </trans-unit>
    </body>
  </file>
</xliff>