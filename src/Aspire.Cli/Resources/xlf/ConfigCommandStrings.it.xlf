      <trans-unit id="ConfigurationKeyDeletedGlobally">
        <source>Configuration '{0}' deleted globally.</source>
        <target state="translated">La configurazione '{0}' è stata eliminata a livello globale.</target>
        <note />
      </trans-unit>
      <trans-unit id="ConfigurationKeyDeletedLocally">
        <source>Configuration '{0}' deleted locally.</source>
        <target state="translated">La configurazione '{0}' è stata eliminata a livello locale.</target>
        <note />
      </trans-unit>
      <trans-unit id="ConfigurationKeySetGlobally">
        <source>Configuration '{0}' set to '{1}' globally.</source>
        <target state="translated">La configurazione '{0}' è impostata su '{1}' a livello globale.</target>
        <note>{0} is the key, {1} is the value</note>
      </trans-unit>
      <trans-unit id="ConfigurationKeySetLocally">
        <source>Configuration '{0}' set to '{1}' locally.</source>
        <target state="translated">Configurazione '{0}' impostata su '{1}' a livello locale.</target>
        <note>{0} is the key, {1} is the value</note>
      </trans-unit>
      <trans-unit id="DeleteCommand_Description">
        <source>Delete a configuration value.</source>
        <target state="translated">Elimina un valore di configurazione.</target>
        <note />
      </trans-unit>
      <trans-unit id="DeleteCommand_GlobalArgumentDescription">
        <source>Delete the configuration value from the global $HOME/.aspire/settings.json instead of the local settings file.</source>
        <target state="translated">Elimina il valore di configurazione dal file globale $HOME/.fqd/settings.json anziché dal file di impostazioni locali.</target>
        <note />
      </trans-unit>
      <trans-unit id="DeleteCommand_KeyArgumentDescription">
        <source>The configuration key to delete.</source>
        <target state="translated">Chiave di configurazione da eliminare.</target>
        <note />
      </trans-unit>
      <trans-unit id="DeleteCommand_PromptForGlobal">
        <source>Delete the configuration value from the global $HOME/.aspire/settings.json instead of the local settings file?</source>
        <target state="translated">Vuoi eliminare il valore di configurazione dal file globale $HOME/.fqd/settings.json anziché dal file di impostazioni locali?</target>
        <note />
      </trans-unit>
      <trans-unit id="DeleteCommand_PromptForKey">
        <source>Which config value do you want to delete?</source>
        <target state="translated">Quale valore di configurazione vuoi eliminare?</target>
        <note />
      </trans-unit>
