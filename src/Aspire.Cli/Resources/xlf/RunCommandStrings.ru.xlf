<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="ru" original="../RunCommandStrings.resx">
    <body>
      <trans-unit id="Dashboard">
        <source>Dashboard</source>
        <target state="new">Dashboard</target>
        <note />
      </trans-unit>
      <trans-unit id="Description">
        <source>Run an Aspire app host in development mode.</source>
        <target state="translated">Запустите хост приложений Aspire в режиме разработки.</target>
        <note />
      </trans-unit>
      <trans-unit id="Endpoints">
        <source>Endpoints</source>
        <target state="translated">Конечные точки</target>
        <note />
      </trans-unit>
      <trans-unit id="Health">
        <source>The health of the resource, eg Healthy</source>
        <target state="translated">Работоспособность ресурса, например "Работоспособен"</target>
        <note />
      </trans-unit>
      <trans-unit id="IsCompatibleAppHostIsNull">
        <source>IsCompatibleAppHost is null</source>
        <target state="translated">Значение параметра IsCompatibleAppHost равно null</target>
        <note />
      </trans-unit>
      <trans-unit id="Logs">
        <source>Logs</source>
        <target state="new">Logs</target>
        <note />
      </trans-unit>
      <trans-unit id="PressCtrlCToStopAppHost">
        <source>Press [bold]Ctrl+C[/] to stop the app host and exit.</source>
        <target state="translated">Нажмите [bold]Ctrl+C[/], чтобы остановить хост приложений и выйти.</target>
        <note>[bold] should not be localized</note>
      </trans-unit>
      <trans-unit id="ProjectArgumentDescription">
        <source>The path to the Aspire app host project file.</source>
        <target state="translated">Путь к файлу проекта хоста приложений Aspire.</target>
        <note />
      </trans-unit>
      <trans-unit id="ProjectCouldNotBeRun">
        <source>The project could not be run. For more information run with --debug switch.</source>
        <target state="translated">Не удалось запустить проект. Для получения дополнительных сведений запустите команду с параметром --debug.</target>
        <note />
      </trans-unit>
      <trans-unit id="Resource">
        <source>Resource</source>
        <target state="translated">Ресурс</target>
        <note>A column header (noun)</note>
      </trans-unit>
      <trans-unit id="StartingAppHost">
        <source>Starting Aspire app host...</source>
        <target state="translated">Запускается хост приложений Aspire...</target>
        <note />
      </trans-unit>
      <trans-unit id="StartingDashboard">
        <source>Starting Aspire dashboard...</source>
        <target state="translated">Запускается панель мониторинга Aspire...</target>
        <note />
      </trans-unit>
      <trans-unit id="State">
        <source>State</source>
        <target state="translated">Состояние</target>
        <note>The state of the resource, eg Running</note>
      </trans-unit>
      <trans-unit id="Type">
        <source>Type</source>
        <target state="translated">Тип</target>
        <note>The type of the resource, eg Project</note>
      </trans-unit>
      <trans-unit id="WatchArgumentDescription">
        <source>Start project resources in watch mode.</source>
        <target state="translated">Запустите ресурсы проекта в режиме наблюдения.</target>
        <note />
      </trans-unit>
    </body>
  </file>
</xliff>