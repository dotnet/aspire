        <source>Display configuration file paths and available features.</source>
        <target state="new">Display configuration file paths and available features.</target>
        <note />
      </trans-unit>
      <trans-unit id="InfoCommand_GlobalSettingsPath">
        <source>Global settings path</source>
        <target state="new">Global settings path</target>
        <note />
      </trans-unit>
