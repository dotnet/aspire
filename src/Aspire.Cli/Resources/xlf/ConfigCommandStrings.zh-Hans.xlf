        <source>Local settings path</source>
        <target state="new">Local settings path</target>
        <note />
      </trans-unit>
      <trans-unit id="InfoCommand_SettingsProperties">
        <source>Settings file properties</source>
        <target state="new">Settings file properties</target>
        <note />
      </trans-unit>
