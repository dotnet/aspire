{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Azure": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Aspire": {
    "RabbitMQ": {
      "Client": {
        "OrderQueueName": "orders"
      }
    },
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectRetry": 2
        }
      }
    }
  }
}
