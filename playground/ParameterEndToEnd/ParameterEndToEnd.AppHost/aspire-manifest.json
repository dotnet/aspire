{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "sql": {
      "type": "parameter.v0",
      "connectionString": "{sql.value}",
      "value": "{sql.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true
        }
      }
    },
    "db": {
      "type": "value.v0",
      "connectionString": "{sql.connectionString};Initial Catalog=db"
    },
    "insertionrows": {
      "type": "parameter.v0",
      "value": "{insertionrows.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    },
    "cs": {
      "type": "value.v0",
      "connectionString": "sql={sql.bindings.tcp.url};rows={insertionrows.value}"
    },
    "parameterFromConnectionStringConfig": {
      "type": "parameter.v0",
      "connectionString": "{parameterFromConnectionStringConfig.value}",
      "value": "{parameterFromConnectionStringConfig.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true
        }
      }
    },
    "throwing": {
      "type": "parameter.v0",
      "value": "{throwing.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    },
    "parameterFromConnectionStringConfigMissing": {
      "type": "parameter.v0",
      "connectionString": "{parameterFromConnectionStringConfigMissing.value}",
      "value": "{parameterFromConnectionStringConfigMissing.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true
        }
      }
    },
    "markdownDescription": {
      "type": "parameter.v0",
      "value": "{markdownDescription.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    },
    "customInput": {
      "type": "parameter.v0",
      "value": "{customInput.inputs.value}",
      "inputs": {
        "value": {
          "type": "string"
        }
      }
    },
    "api": {
      "type": "project.v0",
      "path": "../ParameterEndToEnd.ApiService/ParameterEndToEnd.ApiService.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{api.bindings.http.targetPort}",
        "InsertionRows": "{insertionrows.value}",
        "ConnectionStrings__cs": "{cs.connectionString}",
        "ConnectionStrings__db": "{db.connectionString}",
        "DB_HOST": "{sql.bindings.tcp.host}",
        "DB_PORT": "{sql.bindings.tcp.port}",
        "DB_USERNAME": "sa",
        "DB_PASSWORD": "{sql-password.value}",
        "DB_URI": "mssql://{sql.bindings.tcp.host}:{sql.bindings.tcp.port}/db",
        "DB_JDBCCONNECTIONSTRING": "jdbc:sqlserver://{sql.bindings.tcp.host}:{sql.bindings.tcp.port};trustServerCertificate=true;databaseName=db",
        "DB_DATABASE": "db"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "sql-password": {
      "type": "parameter.v0",
      "value": "{sql-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22,
              "minLower": 1,
              "minUpper": 1,
              "minNumeric": 1
            }
          }
        }
      }
    }
  }
}