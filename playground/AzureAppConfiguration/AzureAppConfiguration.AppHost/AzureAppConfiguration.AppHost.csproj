<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>$(DefaultTargetFramework)</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsAspireHost>true</IsAspireHost>
    <UserSecretsId>46387d6e-1b69-4a93-95b9-d9cbeee8bbec</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\KnownResourceNames.cs" Link="KnownResourceNames.cs" />
  </ItemGroup>

  <ItemGroup>
    <AspireProjectOrPackageReference Include="Aspire.Hosting.AppHost" />
    <AspireProjectOrPackageReference Include="Aspire.Hosting.Azure.AppConfiguration" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AzureAppConfiguration.Web\AzureAppConfiguration.Web.csproj" />
  </ItemGroup>

</Project>
