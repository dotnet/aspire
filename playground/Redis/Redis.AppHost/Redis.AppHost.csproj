    <AspireProjectOrPackageReference Include="Aspire.Hosting.Garnet" />
    <AspireProjectOrPackageReference Include="Aspire.Hosting.Valkey" />
    <ProjectReference Include="..\Redis.ApiService\Redis.ApiService.csproj" />
  </ItemGroup>

</Project>
